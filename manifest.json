{
    "manifest_version": 3,
    "name": "Doto",
    "version": "2025.01.12",
    "key": "jcglojmlacpdnpjalikknfjmfcoeofmc",
    "description": "Local first TODO organizer",
    "permissions": [
        "storage"
    ],
    "action": {
        "default_popup": "add.html",
        "default_icon": {
            "16": "icons/doto.png"
        }
    },
    "icons": {
        "16": "icons/doto.png"
    },
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+U",
                "mac": "Command+Shift+U"
            },
            "description": "Open add dialog"
        }
    }
}